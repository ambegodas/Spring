<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
   <bean id="triangle" class="com.sample.beanfactory.Triangle">
    <property name="type" value="equalateral"></property>
   </bean>
   
   <bean id="triangle1" class="com.sample.beanfactory.Triangle">
    <constructor-arg value="Equalateral"></constructor-arg>
    <constructor-arg value="4"></constructor-arg>
   </bean>
   
     <bean id="triangle2" class="com.sample.beanfactory.Triangle">
     <constructor-arg type="int" value="4"></constructor-arg>
   </bean>
   
     <bean id="triangle3" class="com.sample.beanfactory.Triangle">
     <constructor-arg index="0" value="Equalateral"></constructor-arg>
    <constructor-arg index="1" value="4"></constructor-arg>
    </bean>
   
</beans>